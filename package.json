{
  "name": "react-tribute",
  "version": "1.0.0",
  "description": "React component for mentioning using Zurb's Tribute library",
  "main": "dist/index.js",
  "scripts": {
    "run": "babel src -d dist && babel example/example.js --out-file example/index.js",
    "//": "you can also run `npm run test -- -w` to watch",
    "test": "mocha --require test/config.js --recursive test/spec",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./example"
  },
  "author": "Ivan Montiel <imontiel@bolste.com> (https://github.com/imontiel)",
  "license": "MIT",
  "dependencies": {
    "tributejs": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "deepequal": "^0.0.1",
    "enzyme": "^2.6.0",
    "faker": "^3.1.0",
    "jsdom": "8.2.0",
    "mocha": "^3.1.2",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.0.0",
    "react-textarea-autosize": "^4.0.5",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
